                "consumes": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "reportTrackingEvent",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "required": [
                                "method",
                                "id",
                                "jsonrpc",
                                "params"
                            ],
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "required": true,
                                    "description": "请求id，建议使用uuid"
                                },
                                "jsonrpc": {
                                    "type": "string",
                                    "required": true,
                                    "description": "JSONRPC 协议规定，此处必须为2.0",
                                    "default": "2.0"
                                },
                                "method": {
                                    "type": "string",
                                    "required": true,
                                    "description": "调用方法名 此处必须为reportTrackingEvent",
                                    "default": "reportTrackingEvent"
                                },
                                "params": {
                                    "type": "object",
                                    "required": [
                                        "parcelNo",
                                        "trackingEvent"
                                    ],
                                    "properties": {
                                        "parcelNo": {
                                            "type": "object",
                                            "description": "包裹编号",
                                            "properties": {
                                                "value": {
                                                    "type": "string"
                                                },
                                                "type": {
                                                    "type": "string",
                                                    "enum": [
                                                        "PARCEL_NO",
                                                        "ORDER_NO",
                                                        "TRACKING_NO"
                                                    ]
                                                }
                                            }
                                        },
                                        "trackingEvent": {
                                            "type": "object",
                                            "description": "物流轨迹事件信息",
                                            "properties": {
                                                "code": {
                                                    "type": "string",
                                                    "description": "轨迹代码: \n UNKNOWN 未知, COLLECT 已拣货, SHIPPED 已发货, TAKEOFF 已起运, LANDING 已着陆, HANDOVER 已提货, CR_START 开始清关, CR_END 清关结束, DELIVERING 配送中, SINGED 已签收. \n (如果找不到对应代码，请使用UNKNOWN) ",
                                                    "enum": [
                                                        "UNKNOWN",
                                                        "COLLECT",
                                                        "SHIPPED",
                                                        "TAKEOFF",
                                                        "LANDING",
                                                        "HANDOVER",
                                                        "CR_START",
                                                        "CR_END",
                                                        "DELIVERING",
                                                        "SINGED"
                                                    ]
                                                },
                                                "message": {
                                                    "type": "string",
                                                    "description": "轨迹文本信息"
                                                },
                                                "time": {
                                                    "type": "string",
                                                    "description": "轨迹事件发生时间，请使用UTC时间",
                                                    "format": "date-time"
                                                },
                                                "location": {
                                                    "type": "string",
                                                    "description": "轨迹发生所在地点，如城市: 上海"
                                                },
                                                "remark": {
                                                    "type": "string",
                                                    "description": "轨迹事件备注信息"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "response",
                        "schema": {
                            "type": "object",
                            "required": [
                                "id",
                                "jsonrpc"
                            ],
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "required": true,
                                    "description": "请求id"
                                },
                                "jsonrpc": {
                                    "type": "string",
                                    "required": true,
                                    "description": "JSONRPC 协议规定，此处必须为2.0",
                                    "default": "2.0"
                                },
                                "result": {
                                    "type": "null",
                                    "required": true
                                },
                                "error": {
                                    "type": "object",
                                    "required": [
                                        "code",
                                        "message"
                                    ],
                                    "description": "RPC出错信息，error和result同时只能出现一个",
                                    "properties": {
                                        "code": {
                                            "type": "integer",
                                            "required": true,
                                            "description": "错误代码，详情请查看附录错误代码表"
                                        },
                                        "message": {
                                            "type": "string",
                                            "required": true,
                                            "description": "错误描述"
                                        },
                                        "data": {
                                            "type": "any",
                                            "description": "错误数据"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
