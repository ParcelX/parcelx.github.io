                            "properties": {
                                "id": {
                                    "type": "string",
                                    "required": true,
                                    "description": "请求id"
                                },
                                "jsonrpc": {
                                    "type": "string",
                                    "required": true,
                                    "description": "JSONRPC 协议规定，此处必须为2.0",
